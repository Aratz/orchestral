{
    "cell_executable": "modern_egfrd/build/bin/RunGfrd --custom -seed {seed} -e {end_time} -wsize {wsize} -in {input_file} -out {output_file}",
    "signaling_executable": "python3 signaling/signaling.py -seed {seed} -e {end_time} -in {input_file} -out {output_file}",
    "translation_X2S":"python3 translation_X2S.py {config_file} {network_file} {cell_output_files} {signaling_input_file}",
    "translation_S2X":"python3 translation_S2X.py {config_file} {network_file} {target_cell_output_file} {signaling_files} {target_cell_input_file}",
    "data_folder":"data/",
    "cell_file":"cell-{step}.{substep}-{cell_id}",
    "signaling_file":"signaling-{step}.{substep}-({cell_id},{neighbor_id})",
    "network_file":"network-{step}",
    "n_mech_steps":10,
    "n_kin_substeps":10,
    "simulation_time":100
}
